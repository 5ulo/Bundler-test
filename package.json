{
    "name": "wpbs5",
    "version": "5.3.0",
    "description": "Bootstrap 5.3 template",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode=development --stats-error-details",
        "start-idsk": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode=development --env theme=idsk",
        "dev": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js --mode=development",
        "build": "NODE_ENV=production webpack --config ./webpack.config.js --mode=production",
        "build-idsk": "NODE_ENV=production webpack --config ./webpack.config.js --mode=production --env theme=idsk",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Enjoy :)",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/preset-env": "^7.23.3",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "expose-loader": "^4.1.0",
        "file-loader": "^6.2.0",
        "glob": "^10.3.10",
        "handlebars": "^4.7.8",
        "handlebars-loader": "^1.7.3",
        "handlebars-webpack-plugin": "^2.2.1",
        "html-bundler-webpack-plugin": "^3.0.3",
        "image-webpack-loader": "^8.1.0",
        "node-sass": "^9.0.0",
        "postcss-loader": "^7.3.3",
        "terser-webpack-plugin": "^5.3.9",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
